let Students = {
  User1: {
    Name: inputevnts.getstring("Enter your name: "),
    Age: inputevnts.getnumber("Enter your Age: "),
    Grade: inputevnts.getnumber("Enter your Grade: "),
    calc,
  },

  User2: {
    Name: inputevnts.getstring("Enter your name: "),
    Age: inputevnts.getnumber("Enter your Age: "),
    Grade: inputevnts.getnumber("Enter your Grade: "),
    calc,
  },

  User3: {
    Name: inputevnts.getstring("Enter your name: "),
    Age: inputevnts.getnumber("Enter your Age: "),
    Grade: inputevnts.getnumber("Enter your Grade: "),
    calc,
  },

  User4: {
    Name: inputevnts.getstring("Enter your name: "),
    Age: inputevnts.getnumber("Enter your Age: "),
    Grade: inputevnts.getnumber("Enter your Grade: "),
    calc,
  },
};

function calc(grade) {
  if (grade >= 90) {
    return "A";
  } else if (grade >= 80) {
    return "B";
  } else if (grade >= 70) {
    return "C";
  } else if (grade >= 50) {
    return "D";
  } else {
    return "F";
  }
}
